# Backend API Configuration
# Copy this file to .env and update with your actual values

# Base URL for your backend API
BASE_URL=http://msgapp.local:8080

# Basic Authentication Header (Base64 encoded credentials)
AUTH_HEADER=Basic R2VuZXJhbDpHZW5lcmFs

# API Endpoints
REGISTER_ENDPOINT=/public/signup
LOGIN_ENDPOINT=/oauth/token

# Logging Configuration (optional)
# Options: trace, debug, info, warning, error, none
# Default: trace in dev mode, warning in production mode
# LOG_LEVEL=debug
